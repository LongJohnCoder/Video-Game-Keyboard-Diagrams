# Deny all PHP files
<FilesMatch "\.php$">
Order deny,allow
Deny from all
</FilesMatch>

# Allow specific PHP files
<FilesMatch "^keyboard-recaptcha\.php$">
Order deny,allow
Allow from all
</FilesMatch>
<FilesMatch "^keyboard-sitemap\.php$">
Order deny,allow
Allow from all
</FilesMatch>
<FilesMatch "^keyboard-init\.php$">
Order deny,allow
Allow from all
</FilesMatch>
