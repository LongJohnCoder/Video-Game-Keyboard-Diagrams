IndexIgnore *
<Files "keyboard-connection.php">
    Order Allow,Deny
    Deny from all
</Files>
Redirect 301 /keyboard/keyboardchart.php /keyboard/keyboard-chart.php
Redirect 301   /keyboard/keyboard.js.php    /keyboard/keyboard-js.php
Redirect 301  /keyboard/keyboardlist.php  /keyboard/keyboard-list.php
Redirect 301   /keyboard/keyboardlog.php   /keyboard/keyboard-log.php
Redirect 301  /keyboard/keyboardwiki.php  /keyboard/keyboard-wiki.php
RewriteEngine on
RewriteRule ^chart-(.*)\.php$ http://isometricland.net/keyboard/keyboard-diagram-$1.php [R=301,L]
RewriteRule   ^kbd-(.*)\.svg$ http://isometricland.net/keyboard/keyboard-diagram-$1.svg [R=301,L]
RewriteRule ^keyboard-diagram-(.*)\.php$ ./keyboard-chart.php?seo=$1 [QSA]
RewriteRule ^keyboard-diagram-(.*)\.svg$   ./keyboard-svg.php?seo=$1 [QSA]
